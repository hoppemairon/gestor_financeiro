      "interpretacao": "Receita média por procedimento realizado"
    },
    {
      "nome": "Ticket Médio Paciente",
      "formula": "receita_total / numero_pacientes",
      "unidade": "R$/paciente",
      "interpretacao": "Valor médio gasto por paciente"
    },
    {
      "nome": "Taxa de Ocupação Consultório",
      "formula": "horas_utilizadas / horas_disponiveis * 100",
      "unidade": "%",
      "interpretação": "Percentual de ocupação dos consultórios"
    },
    {
      "nome": "Margem por Especialidade",
      "formula": "(receita_especialidade - custo_especialidade) / receita_especialidade * 100",
